{
  "name": "tp-dacs-2022-be",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run open-browser && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "lint-fix": "eslint src --fix && prettier --write \"src/**/*.*\"",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "npm run plop --plopfile ./generators/plopfile.js",
    "open-browser": "start http://localhost:3000 || true",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npm run lint-fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.3.2",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "next": "12.1.6",
    "next-images": "^1.7.0",
    "next-pwa": "^5.5.2",
    "react": "18.1.0",
    "react-bootstrap-typeahead": "^6.0.0-rc.1",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "18.1.0",
    "reactstrap": "^9.0.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/recompose": "^0.30.10",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "axios": "^0.27.2",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "8.0.1",
    "lint-staged": "^12.4.2",
    "plop": "^3.1.0",
    "prettier": "^2.6.2",
    "react-redux": "^8.0.2",
    "recompose": "^0.30.0",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.2.0",
    "redux-responsive": "^4.3.8",
    "redux-thunk": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.52.3",
    "typescript": "^4.7.2",
    "webpack": "5"
  },
  "overrides": {
    "react": "18.1.0"
  }
}
